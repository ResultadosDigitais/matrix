{
  "name": "matrix",
  "version": "1.0.0",
  "description": "The first open virtual office",
  "scripts": {
    "start-backend-development-server": "nodemon --exec babel-node ./backend/index.js",
    "start-frontend-development-server": "webpack --watch --config webpack.development.js",
    "build-backend": "babel ./backend -d dist",
    "build-frontend": "webpack --mode production --config webpack.production.js",
    "start-backend": "NODE_ENV=production node ./dist/index.js",
    "start": "npm-run-all -p start-backend-development-server start-frontend-development-server",
    "test": "nyc --reporter=text mocha --require @babel/register './test/**/*.test.js' --exit",
    "watch-test": "npm run test -- --watch --recursive",
    "cover": "nyc report --reporter=html --reporter=text --reporter=lcovonly --report-dir=test/coverage;",
    "lint": "eslint './**/*.js'"
  },
  "author": "Juliemar Berri",
  "license": "ISC",
  "dependencies": {
    "@sentry/browser": "^5.1.2",
    "bootstrap": "^4.3.1",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "hashmap": "^2.3.0",
    "jquery": "^3.4.1",
    "md5": "^2.2.1",
    "nodemon": "^1.18.10",
    "popper.js": "^1.15.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-mocha": "^5.3.0",
    "mini-css-extract-plugin": "^0.7.0",
    "mocha": "^6.1.4",
    "mock-local-storage": "^1.1.8",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "sinon": "^7.3.2",
    "style-loader": "^0.23.1",
    "supertest": "^4.0.2",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^4.30.0",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-cli": "^3.3.1",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.2.1"
  }
}
